<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="156dp"
            android:layout_width="176dp"
            android:id="@+id/img_cart"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="44dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="52dp"
            android:layout_width="63dp"
            android:id="@+id/img_phone"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="52dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="180dp"
            android:layout_marginRight="180dp" />
        <Constraint
            android:layout_height="31dp"
            android:layout_width="147dp"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            motion:layout_editor_absoluteY="219dp"
            android:layout_height="30dp"
            android:layout_width="61dp"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="@+id/img_phone"
            motion:layout_constraintEnd_toEndOf="@+id/img_phone"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            motion:layout_editor_absoluteY="257dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/img_photo"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/imageView2"
            motion:layout_constraintHorizontal_bias="0.066" />
        <Constraint
            motion:layout_editor_absoluteY="305dp"
            android:layout_height="37dp"
            android:layout_width="65dp"
            android:id="@+id/imageView8"
            motion:layout_constraintStart_toStartOf="@+id/img_photo"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            motion:layout_editor_absoluteY="371dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/img_gmae"
            motion:layout_constraintStart_toStartOf="@+id/imageView8"
            motion:layout_constraintEnd_toEndOf="@+id/imageView8"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_height="41dp"
            android:layout_width="69dp"
            android:id="@+id/imageView9"
            motion:layout_constraintStart_toStartOf="@+id/img_gmae"
            motion:layout_constraintEnd_toEndOf="@+id/img_gmae"
            motion:layout_constraintBottom_toTopOf="@+id/img_cart"
            android:layout_marginBottom="52dp"
            motion:layout_constraintHorizontal_bias="1.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="135dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="171dp"
            android:id="@+id/img_cart"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="64dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="63dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="46dp"
            android:id="@+id/img_phone"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3" />
    </ConstraintSet>
</MotionScene>
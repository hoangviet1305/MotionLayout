<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/photo"
               motion:framePosition="0"
               android:alpha="0.5"
               android:rotation="-25"
               android:rotationX="-113"
               android:scaleY="0"
               android:rotationY="82" />
           <KeyAttribute
               motion:motionTarget="@+id/head"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/game"
               motion:framePosition="0"
               android:alpha="0.5"
               android:rotationX="8"
               android:rotationY="162" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="57dp"
            android:layout_width="184dp"
            android:id="@+id/imageView11"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="36dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            motion:layout_editor_absoluteY="576dp"
            android:layout_height="112dp"
            motion:layout_editor_absoluteX="120dp"
            android:layout_width="152dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:layout_height="69dp"
            android:layout_width="84dp"
            android:id="@+id/photo"
            motion:layout_constraintTop_toBottomOf="@+id/imageView11"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="28dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            motion:layout_editor_absoluteY="222dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="260dp"
            android:layout_height="68dp"
            android:layout_width="64dp"
            android:id="@+id/head"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.512" />
        <Constraint
            motion:layout_editor_absoluteY="349dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView14"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="385dp"
            android:layout_height="56dp"
            android:layout_width="73dp"
            android:id="@+id/game"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="485dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/head"
            android:layout_width="64dp"
            android:layout_height="68dp"
            motion:layout_editor_absoluteY="260dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.512"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="121dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="151dp"
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="44dp"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_editor_absoluteY="566dp" />
        <Constraint
            android:layout_height="49dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="184dp"
            android:id="@+id/imageView11"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"
            motion:layout_constraintHorizontal_bias="0.505" />
        <Constraint
            motion:layout_editor_absoluteY="253dp"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="175dp"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView14"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="144dp"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"
            android:layout_width="wrap_content"
            android:id="@+id/photo"
            motion:layout_constraintTop_toTopOf="@+id/imageView5"
            motion:layout_constraintVertical_bias="0.475"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="172dp"
            android:layout_marginRight="172dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="140dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"
            android:layout_width="wrap_content"
            android:id="@+id/head"
            motion:layout_constraintTop_toTopOf="@+id/imageView5" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="135dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"
            android:layout_width="wrap_content"
            android:id="@+id/game" />
    </ConstraintSet>
</MotionScene>